    "from bs4 import BeautifulSoup\n",
    "import requests\n",
    "\n",
    "def wiki_header(url):\n",
    "    response = requests.get(url)\n",
    "    page = BeautifulSoup(response.text, 'html.parser')\n",
    "    return page.find('h1').text\n"
   ]
  },
